{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "resolveJsonModule": true,
    "types": [
      "node",
      "bun-types"
    ],
    "paths": {
      "unplugin": [
        "./src/index.ts"
      ]
    }
  },
  "include": [
    "src",
    "test"
  ]
}
