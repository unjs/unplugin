{
  "name": "docs",
  "type": "module",
  "private": true,
  "scripts": {
    "gen-files": "tsx ./.vitepress/data/gen-files.ts",
    "docs:dev": "vitepress dev --open",
    "docs:build": "pnpm gen-files && vitepress build",
    "lint": "eslint . && case-police '**/*.md'",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@iconify-json/heroicons": "^1.1.19",
    "@iconify-json/lucide": "^1.1.160",
    "@iconify-json/radix-icons": "^1.1.14",
    "@iconify-json/simple-icons": "^1.1.90",
    "@types/node": "^20.11.10",
    "case-police": "^0.6.1",
    "consola": "^3.2.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "markdown-it": "^14.0.0",
    "markdown-it-github-alerts": "^0.3.0",
    "ofetch": "^1.3.3",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.4",
    "unplugin": "^1.6.0",
    "unplugin-icons": "^0.18.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vitepress": "1.0.0-rc.40",
    "vitepress-plugin-twoslash": "^0.10.2",
    "vue": "^3.4.15"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint"
  }
